	using ::clearerr;
	using ::feof;
	using ::ferror;
	using ::fflush;
	using ::fileno;
	using ::fprintf;
	using ::fputc;
	using ::fputs;
	using ::fwrite;
	using ::printf;
	using ::putchar;
	using ::puts;
	using ::snprintf;
	using ::sprintf;
	using ::vfprintf;
	using ::vprintf;
	using ::vsnprintf;
	using ::vsprintf;
}
}